{
	"expo": {
		"name": "EzzRide",
		"slug": "EzzRide",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "ezzride",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#2f80ed"
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.ezzzat.EzzRide"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#2f80ed"
			},
			"package": "com.ezzzat.EzzRide"
		},
		"web": {
			"bundler": "metro",
			"output": "server",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [["expo-router", { "origin": "https://ezzride.com" }]],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"eas": {
				"projectId": "1730542e-0c59-44c2-8fd5-8d47c9f9b71d"
			}
		}
	}
}
